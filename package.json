{
  "name": "dots-wrapper",
  "version": "2.1.0",
  "description": "Digital Ocean API V2 wrapper. TypeScript - Javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "nyc jasmine-ts \"spec/digitalOcean.spec.ts\"",
    "cover": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pjpimentel/dots.git"
  },
  "author": "Pedro J. Pimentel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pjpimentel/dots/issues"
  },
  "homepage": "https://github.com/pjpimentel/dots",
  "dependencies": {
    "axios": "^0.16.2",
    "rxjs": "^6.1.0"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^7.0.55",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.6",
    "typedoc": "^0.11.1"
  },
  "nyc": {
    "extension": [
      "*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "dist/src/common",
      "coverage"
    ],
    "reporter": [
      "text",
      "html"
    ],
    "all": true
  }
}
