{
  "name": "dots-wrapper",
  "version": "2.1.0",
  "description": "Digital Ocean API V2 wrapper. TypeScript - Javascript",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc",
    "test": "export TS_NODE_FAST=YES && nyc jasmine-ts --config=spec/support/jasmine.json \"spec/digitalOcean.spec.ts\"",
    "cover": "nyc npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pjpimentel/dots.git"
  },
  "author": "Pedro J. Pimentel",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pjpimentel/dots/issues"
  },
  "homepage": "https://github.com/pjpimentel/dots",
  "dependencies": {
    "axios": "^0.16.2",
    "rxjs": "^6.2.1"
  },
  "devDependencies": {
    "@types/jasmine": "^2.8.8",
    "@types/node": "^10.3.3",
    "jasmine-spec-reporter": "^4.2.1",
    "jasmine-ts": "^0.2.1",
    "nyc": "^12.0.2",
    "source-map-support": "^0.5.6",
    "ts-node": "^6.1.1",
    "typedoc": "^0.11.1",
    "typescript": "^2.9.2"
  },
  "nyc": {
    "include": [
      "src/**/*.ts",
      "src/**/*.tsx"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "text",
      "html",
      "lcov"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
